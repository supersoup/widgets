<div class="date-header">
    <a class="date-header-previous" href="javascript:void(0);">上月</a>
    <a class="date-header-next" href="javascript:void(0);">下月</a>
    <div class="date-header-select">
        <select class="date-header-years">
            <% for (var k = renderYear - 20; k < renderYear; k ++) { %>
            <option value="<%= k %>"><%= k %></option>
            <% } %>
            <option value="<%= renderYear %>" selected><%= renderYear %></option>
            <% for (var l = renderYear + 1; l < renderYear + 20; l ++) { %>
            <option value="<%= l %>"><%= l %></option>
            <% } %>
        </select>
        年
        <select class="date-header-months">
            <option value="1" <%if (renderMonth + 1 === 1) {%> selected <% }%> >1</option>
            <option value="2" <%if (renderMonth + 1 === 2) {%> selected <% }%> >2</option>
            <option value="3" <%if (renderMonth + 1 === 3) {%> selected <% }%> >3</option>
            <option value="4" <%if (renderMonth + 1 === 4) {%> selected <% }%> >4</option>
            <option value="5" <%if (renderMonth + 1 === 5) {%> selected <% }%> >5</option>
            <option value="6" <%if (renderMonth + 1 === 6) {%> selected <% }%> >6</option>
            <option value="7" <%if (renderMonth + 1 === 7) {%> selected <% }%> >7</option>
            <option value="8" <%if (renderMonth + 1 === 8) {%> selected <% }%> >8</option>
            <option value="9" <%if (renderMonth + 1 === 9) {%> selected <% }%> >9</option>
            <option value="10" <%if (renderMonth + 1 === 10) {%> selected <% }%> >10</option>
            <option value="11" <%if (renderMonth + 1 === 11) {%> selected <% }%> >11</option>
            <option value="12" <%if (renderMonth + 1 === 12) {%> selected <% }%> >12</option>
        </select>
        月
    </div>
</div>

<div class="date-body">
    <table class="date-body-table">
        <tr class="date-week-row">
            <th>日</th>
            <th>一</th>
            <th>二</th>
            <th>三</th>
            <th>四</th>
            <th>五</th>
            <th>六</th>
        </tr>

        <tbody class="date-list">
        <% for (var i = 0; i < renderList.length; i = i + 7) { %>
        <tr class="date-date-row">
            <% for (var j = i; j < i + 7; j ++) { %>
            <td
                class="<% if (renderList[j].monthIs === 'prev') { %>date-month-prev<% } %>
                       <% if (renderList[j].monthIs === 'current') { %>date-month-current<% } %>
                       <% if (renderList[j].monthIs === 'next') { %>date-month-next<% } %>
                       <% if (renderList[j].year == inputYear && renderList[j].month == inputMonth && renderList[j].date == inputDate) { %>date-input-current<% } %>
                       <% if (renderList[j].year == thisYear && renderList[j].month == thisMonth && renderList[j].date == thisDate) { %>date-today<% } %>"
            >
                <%= renderList[j].date %>
            </td>
            <% } %>
        </tr>
        <% } %>
        </tbody>

    </table>
</div>